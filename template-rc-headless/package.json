{
  "name": "template-rc-headless",
  "version": "0.0.0",
  "description": "Template for react component using babel.",
  "keywords": [],
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justnewbee/templates.git"
  },
  "homepage": "https://github.com/justnewbee/templates/template-rc-headless",
  "bugs": "https://github.com/justnewbee/templates/issues",
  "author": {
    "name": "Jianchun Wang",
    "email": "justnewbee@gmail.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@kcuf/ts-config": "^0.0.1",
    "@types/react": "^18.3.23",
    "cross-env": "^10.0.0",
    "react": "^18.3.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.x",
    "react": "^18.x"
  },
  "dependencies": {
    "@kcuf-hook/use-is-unmounted": "^0.0.1",
    "immer": "^10.1.1"
  },
  "scripts": {
    "build:clean": "rimraf dist",
    "build:esm": "cross-env ESM=1 babel src -d dist/esm --extensions .ts,.tsx --source-maps",
    "build:cjs": "cross-env ESM=0 babel src -d dist/cjs --extensions .ts,.tsx",
    "build:types": "tsc -rootDir src --outDir dist/types --declaration --noEmit false --emitDeclarationOnly --isolatedModules false",
    "build": "pnpm build:esm && pnpm build:cjs && pnpm build:types",
    "watch": "pnpm build:esm -w",
    "prepublishOnly": "pnpm build:clean && pnpm build"
  }
}
